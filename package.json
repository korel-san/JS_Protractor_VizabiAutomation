{
  "name": "vizabi-protractor-tests",
  "version": "0.0.1",
  "private": true,
  "description": "Protractor tests for Vizabi tools page",
  "main": "",
  "scripts": {
    "pretest": "npm run lint:eslint &&./node_modules/.bin/gulp tslint",
    "test": "npm run eslint && npm run karma-local",
    "lint:eslint": "./node_modules/.bin/eslint --ignore-path .gitignore  --ext js --fix . .config",
    "karma-local": "NODE_ENV=test ./node_modules/.bin/karma start .config/karma.conf.js",
    "karma-sauce": "NODE_ENV=test ./node_modules/.bin/karma start .config/karma-sauce.conf.js",
    "karma-bstack": "NODE_ENV=test ./node_modules/.bin/karma start .config/karma-browserstack.conf.js",
    "bstack-tunnel": ".config/tools/bstack-local.js",
    "update-webdriver": "./node_modules/protractor/bin/webdriver-manager update",
    "selenium-server-standalone": "./node_modules/protractor/bin/webdriver-manager start",
    "e2e": "./node_modules/.bin/protractor ./.config/protractor-local.conf.js",
    "e2e:local": "./node_modules/.bin/protractor ./.config/protractor-local.conf.js",
    "e2e:bstack": "node .config/tools/wait4tunnel.js && ./node_modules/.bin/protractor ./.config/protractor-browserstack.conf.js",
    "e2e:sauce": "./node_modules/.bin/protractor ./.config/protractor-sauce.conf.js --elementExplorer"
  },
  "devDependencies": {
    "eslint": "2.3.0",
    "eslint-config-valorsoft": "0.0.8",
    "gulp": "3.9.1",
    "istanbul-instrumenter-loader": "0.1.3",
    "jasmine": "2.4.1",
    "jasmine-allure-reporter": "0.2.3",
    "jasmine-reporters": "2.1.1",
    "jasmine-spec-reporter": "2.4.0",
    "karma": "0.13.19",
    "karma-browserstack-launcher": "0.1.9",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.7",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sauce-launcher": "0.3.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "1.7.0",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "2.1.4",
    "pre-commit": "1.1.2",
    "protractor": "3.1.1",
    "wd": "0.4.0",
    "webpack": "1.12.14"
  }
}
